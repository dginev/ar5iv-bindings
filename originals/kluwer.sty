\def\scan#1#2\end{\def\aux{#1}%
  \ifklaar\global\setbox\bdj=\hbox{#1#2\space}%
\else%
\ifx\aux\empty%
\else%
\def\aux{#2}%
\onestep{#1}%
\ifx\aux\empty%
\else%
\scan#2\end%
\fi\fi\fi}
%
\def\onestep#1{\ifklaar\else%
	       \if#1){)}\klaartrue\else%
	       \if#1.{.}\klaartrue\else%
	       \if#1]{\]}\klaartrue\else%
	       \if#1-{--}\klaartrue\else%
	       \if#1*{*}\klaartrue\else%
	       #1%
	       \fi\fi\fi\fi\fi\fi}
%
\def\subspitem#1{\scan#1\end}
%
\def\spitem#1 {\global\setbox\bdj\hbox{}%
	       \klaarfalse%
	       \expandafter\item[\subspitem{#1}]%
	       \unhbox\bdj}
