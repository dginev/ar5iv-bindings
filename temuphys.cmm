%
% \begdoublecolumns and \enddoublecolumns start and end text
% passages in twocolumn format
\def\begdoublecolumns{\begingroup
  \output={\global\setbox\partialpage=\vbox{\unvbox255}}\eject
  \output={\doublecolumnout} \hsize=\pagewidth
  \advance\hsize by-\indexcolgap\divide\hsize by 2
  \vsize=2\pageheight\advance\vsize by1cm\relax}
