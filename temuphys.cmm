\vbox{\vskip\topskip\hrule height\z@
         \nobreak\vskip -\topskip\vskip-9pt
         \petit#2
%        \hrule width\tableseprulewidth
         \kern#1true cm\relax
%        \rahmen{#1}%
%        \hrule width\tableseprulewidth
         \vskip\medskipamount}%
\endinsert}
